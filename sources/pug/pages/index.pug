					.owl-carousel(data-items='1', data-sm-items='2' data-md-items='3' data-lg-items='5' data-stage-padding='0', data-loop='true', data-margin='30', data-mouse-drag="false" data-nav-custom="#owl-carousel-nav"  data-autoplay='true')
