									include ../elements/svg-icons/_icon-neurology
									h4.block-accent-title Neurology
						.item.wow.fadeInRight(data-wow-delay=".3s")
							a.block-accent(href='#')
								.block-accent-inner
									include ../elements/svg-icons/_icon-traumatology
									h4.block-accent-title Traumatology
						.item.wow.fadeInRight(data-wow-delay=".5s")
							a.block-accent(href='#')
								.block-accent-inner
									include ../elements/svg-icons/_icon-cardiology
									h4.block-accent-title Cardiology
						.item.wow.fadeInRight(data-wow-delay=".7s")
							a.block-accent(href='#')
								.block-accent-inner
									include ../elements/svg-icons/_icon-dentistry
									h4.block-accent-title Dentistry
						.item.wow.fadeInRight(data-wow-delay=".9s")
