extends  ../_skeleton

//- Includes
include ../elements/_news

block variables
	- var activeCategory = 'Home'

block title
	title Home

block page
	// Page Header
	include ../sections/_header-default

	// Swiper

	section.swiper-main-wrap.wow.fadeIn
		.swiper-container.swiper-slider(data-index-bullet="true" data-clickable='true' data-custom-pagination='#swiper-pagination-index' data-slide-effect='fade' data-autoplay='4000' data-simulate-touch="false")
			.swiper-wrapper
				.swiper-slide(data-slide-bg='images/slide-01.jpg')
					.swiper-slide-caption
						.parallax-container.parallax-container-modern(data-parallax-img='')
							.parallax-content.section-lg
								p.heading-4.heading-divider-modern(data-caption-animate='fadeInUp' data-caption-delay='250' style='font-weight: 400') Individual approach to each patient
								h1(data-caption-animate='fadeInUp' data-caption-delay='100') Health Service
								a(href=config.contactLink data-caption-animate='fadeInUp' data-caption-delay='450').button.button-white.button-effect-ujarak make an Appointment
				.swiper-slide(data-slide-bg='images/slide-02.jpg')
					.swiper-slide-caption
						.parallax-container.parallax-container-modern(data-parallax-img='')
							.parallax-content.section-lg
								p.heading-4.heading-divider-modern(data-caption-animate='fadeInUp' data-caption-delay='250' style='font-weight: 400') Committed to excellent and complete patient care
								h1(data-caption-animate='fadeInUp' data-caption-delay='100') Health Clinic
								a(href=config.contactLink data-caption-animate='fadeInUp' data-caption-delay='450').button.button-white.button-effect-ujarak make an Appointment
				.swiper-slide(data-slide-bg='images/slide-03.jpg')
					.swiper-slide-caption
						.parallax-container.parallax-container-modern(data-parallax-img='')
							.parallax-content.section-lg
								p.heading-4.heading-divider-modern(data-caption-animate='fadeInUp' data-caption-delay='250' style='font-weight: 400') A range of programs for women’s healthcare
								h1(data-caption-animate='fadeInUp' data-caption-delay='100') Special Touch
								a(href=config.contactLink data-caption-animate='fadeInUp' data-caption-delay='450').button.button-white.button-effect-ujarak make an Appointment

		.swiper-main-list-wrap
			ul.swiper-main-list
				li: a.icon.fa-facebook.icon-white-lighter-block(href='#')
				li: a.icon.fa-twitter.icon-white-lighter-block(href='#')
				li: a.icon.icon-xxs.fa-google-plus.icon-white-lighter-block(href='#')

		.swiper-pagination.swiper-pagination-index#swiper-pagination-index

	// Opening Hours
	section.section.section-lg.bg-white.wow.fadeIn
		.shell
			.range.range-30
				.cell-sm-5.cell-lg-3.reveal-sm-flex
					.block-modern.block-modern-1.wow.fadeIn(data-wow-delay=".1s")
						.block-modern-title
							h4 Opening Hours
						.work-info
							div
								dl
									dt Monday
									dd 8.00 – 17.00
								dl
									dt Saturday
									dd 9.30 – 17.30
								dl
									dt Sunday
									dd 9.30 – 15.00

				.cell-sm-5.cell-lg-3.reveal-sm-flex
					.block-modern.block-modern-2.wow.fadeIn(data-wow-delay=".3s")
						.block-modern-title
							h4 Doctors’ Timetable
						p View a timetable showing when our doctors are usually available.
						a.button.button-albus.button-effect-ujarak(href='timetable.html')  View TimeTable


				.cell-lg-4.reveal-md-flex
					.block-modern.block-modern-3.wow.fadeIn(data-wow-delay=".5s")
						.block-modern-title
							h4 Make an Appointment
						form.rd-mailform(data-form-output='form-output-global', data-form-type='contact', method='post', action='bat/rd-mailform.php')
							.range.range-xs-center.range-20.range-narrow
								.cell-sm-5
									.form-wrap.form-wrap-validation
										select.form-input.select-filter(data-style='modern' data-placeholder="Select a Department", data-minimum-results-for-search="Infinity", data-constraints="@Required")
											option(label="placeholder")
											option(value="2") Neurology
											option(value="3") Dentistry
											option(value="4") Cardiology
											option(value="5") Pediatrics
											option(value="6") Pulmonology
											option(value="7") Ophthalmology
											option(value="8") Diagnostics

										span.select-arrow

								.cell-sm-5
									.form-wrap.form-wrap-validation
										label.form-label(for='forms-phone') Phone
										input#forms-phone.form-input(type='text', name='phone', data-constraints='@Numeric @Required')


								.cell-sm-5
									.form-wrap.form-wrap-validation
										label.form-label(for='forms-email') E-mail
										input#forms-email.form-input(type='email', name='email', data-constraints='@Email @Required')
								.cell-sm-5
									.form-button
										button.button.button-effect-ujarak.button-albus.button-block.button-square(type='submit') make an Appointment


	// About Clinic
	section.section.section-lg.bg-white.wow.fadeIn
		.shell
			.range.range-md-middle.range-30
				.cell-md-6.wow.fadeIn(data-wow-delay=".3s")
					.image-skew(style='max-width: 803px;')
						.image-skew-inner
							.image-skew-inner-img
								+jpg('careers-01-803x458')
				.cell-md-4
					h2.heading-with-aside-divider.wow.fadeInUp(data-wow-delay=".1s") About Clinic
						span.divider
					p.wow.fadeInUp(data-wow-delay=".3s") The Health private medical clinic specializes in a wide variety of health issues, related to absolutely any age or severity level. Our seasoned team of highly trained physicians and practical nurses will be glad to help you 24/7!
					.range.range-40.range-lg-50.range-lg
						.cell-xs-5
							.icon-unit.wow.fadeInUp(data-wow-delay=".7s")
								.icon-unit-left
									+png('icon-01-34x52')
								.icon-unit-body
									h5 Qualified Doctors
									p 25 years of experience
						.cell-xs-5
							.icon-unit.wow.fadeInUp(data-wow-delay=".75s")
								.icon-unit-left
									+png('icon-02-33x52')
								.icon-unit-body
									h5 Modern Equipment
									p Branded and time tested
						.cell-xs-5
							.icon-unit.wow.fadeInUp(data-wow-delay=".8s")
								.icon-unit-left
									+png('icon-03-39x39')
								.icon-unit-body
									h5 Emergency Help
									p Get help anytime
						.cell-xs-5
							.icon-unit.wow.fadeInUp(data-wow-delay=".85s")
								.icon-unit-left
									+png('icon-04-36x43')
								.icon-unit-body
									h5 Individual Approach
									p Each case is always special

	section.section.bg-gray-bright.section-xs.section-counter.context-dark.text-center
		.section-counter-decoration
		.section-counter-decoration
		.shell
			.range.range-xs-center.range-30
				.cell-xs-5.cell-md-2
					.counter-wrap
						.heading-1
							span.counter(data-step='3000') 10
						p.small YEARS OF EXPERIENCE
				.cell-xs-5.cell-md-2
					.counter-wrap
						.heading-1
							span.counter-preffix 0
							span.counter(data-speed='2500') 7
						p.small expert doctors
				.cell-xs-5.cell-md-2
					.counter-wrap
						.heading-1
							span.counter(data-step='1500') 26
						p.small departments
				.cell-xs-5.cell-md-2
					.counter-wrap
						.heading-1
							span.counter-preffix
							span.counter(data-speed='1300') 12
						p.small medical services

